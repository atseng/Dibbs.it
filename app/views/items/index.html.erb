<div class="container">
    <div class="row">
        <div class="span12">
            <table class= "table">
              <tr>
                <th>Avatar</th>
                <th>Name</th>
                <th>Description</th>
                <th>Value</th>
                <th></th>
                <th></th>
                <th></th>
              </tr>
            <% @items.each do |item| %>
              <tr>
                <td><%= item.name %></td>
                <td><%= item.description %></td>
                <td><%= item.value %></td>
                <td><%= link_to 'Destroy', item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
              </tr>
            <% end %>
            </table>
        </div>
    </div>
</div>


<%= debug(params) if Rails.env.development? %>
