<div class="container2">
  <div class="row">
    <div class="span12">
      <table class= "table">
        <tr>
          <th>Name</th>
          <th>Email</th>
          <th></th>
        </tr>
        <% @users.each do |user| %>
          <tr>
            <td><%= user.name %></td>
            <td><%= user.email %></td>
            <td><%= form_for(:friendship, :url => user_friendships_path(:user_id => current_user.id, :friend_id => user.id)) do |f| %>
             <%= f.submit "Friend Request", :method => "post" %>
             <% end %></td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>



<%= debug(params) if Rails.env.development? %>
